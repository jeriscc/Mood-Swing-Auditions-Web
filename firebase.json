{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/function-get-all",
        "function": "getAuditionees"
      },
      {
        "source": "/function-get**",
        "function": "getAuditionee"
      },
      {
        "source": "/function-post",
        "function": "postAuditionee"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
